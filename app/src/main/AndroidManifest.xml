<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.inspektorat.kadelebak">

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name=".view.kade_support.SupportActivity"></activity>
		<activity android:name=".view.kade_profile.ProfileActivity" />
		<activity android:name=".view.kade_complaint.ComplaintActivity" />
		<activity
			android:name=".view.kade_dashboard.DashboardActivity"
			android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".MainActivity" />
	</application>

</manifest>