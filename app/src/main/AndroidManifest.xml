<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.inspektorat.kadelebak">

	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_logo_app_round"
		android:label="@string/app_name"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name=".view.kade_complaint.view.ContentComplaintActivity"></activity>
		<activity android:name=".view.kade_forum.view.ContentForumActivity" />
		<activity
			android:name=".view.kade_login.LoginActivity"
			android:theme="@style/AppTheme.NoActionBar" />
		<activity
			android:name=".view.kade_splash.SplashActivity"
			android:theme="@style/AppTheme.NoActionBar">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".view.kade_forum.view.CreateForumActivity" />
		<activity android:name=".view.kade_complaint.view.CreateComplaintActivity" />
		<activity android:name=".view.kade_village.view.DetailVillageActivity" />
		<activity
			android:name=".view.kade_village.VillageActivity"
			android:label="@string/label_village" />
		<activity
			android:name=".view.kade_forum.ForumActivity"
			android:label="@string/label_forum" />
		<activity
			android:name=".view.kade_dashboard.DashboardActivity"
			android:label="@string/app_name"
			android:theme="@style/AppTheme.NoActionBar" />
		<activity android:name=".MainActivity" />
	</application>

</manifest>